@{
}



<div class="row" style="padding:10px">
    <div class="col-12">
        <button type="submit" id="btncancel" onclick="showhidediv()" class="btn btn-primary myButton"> << Back</button>
    </div>
    <div class="col-6">
        <h3 id="titledialog" class="js-heading" style="margin-top:13px; font-size:20px;">81-83&nbsp;&nbsp;&gt;&gt;&nbsp;&nbsp;1D</h3>
    </div>
</div>


<div class="row" style="padding-right:10px;padding-left:10px">
    <div class="col-12">
        @foreach (spNextStatus nextstatus in Model)
        {
            <div class="box box-solid bg-yellow js-box_button js-room_status_btns" onclick="ajaxrequest(@nextstatus.ID)" data-value="6" is-last="0" style="background-color: @nextstatus.StatusColor !important;">
                <div class="box-body">
                    <div class="text-center cxm-icon">
                        <span class="fa fa-clock-o "></span>
                    </div>
                    <div class="text-center">@nextstatus.StatusName</div>
                </div>
            </div>
        }
    </div>

</div>
<script type="text/javascript">
    $(document).ready(function () {       //Error happens here, $ is not defined.

        var titleheading = bulidname + '&nbsp;&nbsp;&gt;&gt;&nbsp;&nbsp;' + Roomname;

        //alert(bulidname);
        $('#titledialog').html(titleheading)

    });


    

</script>



